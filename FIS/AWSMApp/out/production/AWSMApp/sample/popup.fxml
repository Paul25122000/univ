<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.popupController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Pane fx:id="imageHolder" layoutX="32.0" layoutY="75.0" prefHeight="130.0" prefWidth="130.0" />
            <VBox fx:id="descriptionVBOX" layoutX="186.0" layoutY="75.0" prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 10;">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <right>
      <Pane prefHeight="400.0" prefWidth="31.0" BorderPane.alignment="CENTER">
         <children>
            <VBox fx:id="closeVBOX" prefHeight="200.0" prefWidth="32.0">
               <children>
                  <Button fx:id="close" minWidth="33.0" mnemonicParsing="false" onAction="#closePOP" prefHeight="32.0" prefWidth="33.0" text="x" />
               </children>
            </VBox>
         </children>
      </Pane>
   </right>
</BorderPane>
